.important{height:200px;height:450px; position:absolute; top:50px; left:20; width:410px; background-color:#fff; display:block;}

.order-left{float:left; width:370px;}
.order-left > div.sibebox > div.navs{height: 46px;}
.order-left > div.sibebox > div.navs div{float:left; width:90px; height:46px; line-height:52px; text-align:center; font-size:16px; color:#333; overflow:hidden;}
.order-left > div.sibebox > div.navs li{width:120px; height:30px; line-height:30px; padding:0; text-align:center;}
.order-left > div.sibebox > div.navs li a {height:30px; line-height:30px; padding-top:0px; font-size:16px;width:120px; border:0;}
.order-left  div.sibebox  div.content div.login_div_conbox ul.login_error li.error div{top:-18px; left:10px;}
.order-left  div.sibebox  div.content div.login_div_conbox{padding-top:10px;}
div.login_div_conbox li input.ico_khjl{background:url(../images/login_ico.png) 7px -164px no-repeat;}
.order-left  div.sibebox  div.content div.login_div_conbox li input[type="text"].none_bg_style{width:116px; background:none;}
.order-left  div.sibebox  div.content div.login_div_conbox li select{width:153px; height:40px; line-height:40px; border:1px solid #b4b9bd;}
.order-right{float:left; width:270px; border-left:1px solid #ccc; margin-top:50px; height:350px; margin-left:30px;}
.order-right p{padding:15px 0 45px 40px; font-size:16px;}
.order-right span{display:block; background:url(../images/list_photo_400_bg.png) 40px 5px no-repeat; padding-left:125px; line-height:26px;}
.order-right span h3{font-size:18px; color:#f60;}
.order-right span strong{color:#090; font-size:20px;}

.top_public_three_left{float:left; height:37px; line-height:37px; width:290px;color:#ff9000; background:url(../images/ico_tixiang.png) left center no-repeat; padding-left:25px;}
/**pop**/
/**popbox***/
#index_bg{width:100%;height:100%;top:0px;left:0px; position:absolute;filter: Alpha(opacity=70);opacity:0.7; background:#000000; display:block; z-index:2000;}
#index_popbox{position:absolute; z-index:5000; display:block; position:relative;width:790px; height:448px;}

.index_box_tanchu{border:0px solid #4b4b4b; background:url(../images/index_pop_bg.png) left top no-repeat; padding-top:5px;}
.index_tanchu_delete{float:right; display:block; width:50px; height:50px; background:url(../images/index_pop_guanbi.png) 0px 0px no-repeat; position:absolute; right:-15px; top:-15px; z-index:1000;}
.index_tanchu_delete:hover{background:url(../images/index_pop_guanbi.png) 0px 0px no-repeat;}
.index_popdiv_con{width:570px; height:200px; margin:123px 0 0 208px;}
.index_popdiv_con h2{color:#c02a00; text-align:center; padding:0; margin:0 0 30px 0; display:block; font-family:'Microsoft YaHei'; font-size:24px;}
.index_popdiv_con span{color:#ec4818; font-weight:bolder;}
.index_popdiv_con p{font-size:16px; line-height:30px;}
#scrollText {width: 290px; margin-top:7px;	margin-right: auto;	margin-left: auto;}