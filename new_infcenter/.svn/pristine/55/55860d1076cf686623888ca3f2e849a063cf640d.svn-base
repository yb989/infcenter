ul#navmenu {
	margin: 0;
	border: 0 none;
	padding: 0;
	list-style: none;
	height: 37px;
	
}
ul#navmenu li {
	margin: 0;
	border: 0 none;
	padding: 0;
	float: right; /*For Gecko*/
	display: inline;
	list-style: none;
	position: relative;
	height: 37px;
}
ul#navmenu ul {
	margin: 0;
	border: 0 none;
	padding: 0;
	width: 84px;
	list-style: none;
	display: none;
	position: absolute;
	top: 37px;
	left: 0;
}
ul#navmenu ul li {
	float: none; /*For Gecko*/
	display: block !important;
	display: inline; /*For IE*/
	height: 26px;
}
/* Root Menu */ 
ul#navmenu a {
	padding: 0px 18px 0px 18px;
	float: none !important; /*For Opera*/
	float: left; /*For IE*/
	display: block;
	color: #afafaf;
	font: 12px/22px Verdana, Arial, Helvetica, sans-serif;
	text-decoration: none;
	height: auto !important;
	/*background:url(../images/top_public_one_right_bg.png) left 13px no-repeat;*/
	height:37px;
	line-height:37px;
}
ul#navmenu li li a{height:26px; line-height:26px;}
/* Root Menu Hover Persistence */ 
ul#navmenu a:hover, ul#navmenu li:hover a{
	/* background:url(../images/top_public_one_right_bg.png) left 13px no-repeat; */
	color:#d6d6d6;
	height:37px;
	line-height:37px;
	text-decoration:underline;
}
ul#navmenu li li:hover a{height:26px; line-height:26px;}

/* 2nd Menu */ 
ul#navmenu li:hover li a{
	float: none;
	background: #EEE;
	color: #afafaf;
	padding-left:5px;
	padding-right:5px;
	height:26px; line-height:26px;
}
/* 2nd Menu Hover Persistence */ 
ul#navmenu li:hover li a:hover, ul#navmenu li:hover li:hover a{
	background: #CCC;
	color: #FFF;
}

/* Do Not Move - Must Come Before display:block for Gecko */ 
ul#navmenu li:hover ul ul {
	display: none;
}
ul#navmenu li:hover ul {
	display: block;
}
ul#navmenu li.userName_box{ padding:0;}
ul#navmenu li.userName_box div{display:block;  background:none; text-align:right; padding-right:10px; color:#afafaf;}
ul#navmenu li.userName_box div span{color:#f7b400;}
