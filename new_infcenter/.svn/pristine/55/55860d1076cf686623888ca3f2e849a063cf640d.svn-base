/* CSS Document */
.accordion,
.accordion ul,
.accordion li,
.accordion a,
.accordion span {
	margin: 0;
	padding: 0;
	border: none;
	outline: none;
}

.accordion li {
	list-style: none;
	position:relative;
}

/* Layout & Style */

.accordion li > a {
	display: block;
	position:absolute;
    line-height:32px;
	height: 32px;
	width:70px;
	text-align:center;
	color:#fff;
	font:12px/32px "Microsoft YaHei";
	text-decoration: none;
	background: #204f80;
	top:17px; 
	right:50px;
}

.accordion li > a span, .accordion2 li > a span{font-weight:lighter; padding-right:15px;}

.accordion > li:hover > a,
.accordion > li:target > a,
.accordion > li > a.active {
	color: #fff;
	background: #072d55;
	
}



/* Images */

.accordion > li > a:before, .accordion2 > li > a:before{
	position: absolute;
	top: 0;
	left: 0;
	content: '';
	width: 24px;
	height: 24px;
	margin: 4px 0px;
	background:url(../images/ico_sanjiao.gif) 10px center no-repeat;
	
}




/* Functionality */

.accordion li > .sub-menu{
	display: none;
	padding:5px 10px 0 10px;
	width:680px;
	margin:15px 0 15px 25px;
}

.accordion li > .sub-menu div{border:0px;}

.accordion li > .sub-menu div li{height:40px; }
.accordion li > .sub-menu div li.error_box div{top:7px;}
.accordion li > .sub-menu div.jiaoyimima_box li{padding-left:120px;}
.accordion li > .sub-menu div.jiaoyimima_box li.error_box div{left:420px;}
.accordion li > .sub-menu div.jiaoyimima_box li label{display:block; width:120px; position:absolute; top:6px; margin-left:-120px;}

.accordion li:target > .sub-menu {
	display: block;
}

